{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
    },
    "plugins": [
        "html"
    ],
    "settings": {
        "html/html-extensions": [".html"],
        "html/indent": "+2",
        "html/report-bad-indent": "error",
    }
}
